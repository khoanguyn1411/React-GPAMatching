{
  "name": "gpa-matching",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier --write \"./src/**/*.{html,css}\" --config ./.prettierrc",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "fix-all": "yarn lint && yarn format",
    "prepare": "husky install",
    "docker-destroy": "sudo docker rmi -f gpa-matching-frontend-reactjs",
    "docker-stop": "sudo docker stop gpa-matching-frontend-webreactjs-1",
    "docker-start": "sudo COMPOSE_DOCKER_CLI_BUILD=1 docker compose up -d",
    "docker-reset": "yarn docker-destroy && yarn docker-start"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "yarn fix-all"
    ]
  },
  "dependencies": {
    "@artsy/fresnel": "^6.1.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@tanstack/react-query": "^4.22.0",
    "animate.css": "^4.1.1",
    "axios": "^1.2.2",
    "dayjs": "^1.11.7",
    "jotai": "^1.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.5",
    "react-router-dom": "^6.6.1",
    "yarn": "^1.22.19",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.3",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.3"
  }
}
